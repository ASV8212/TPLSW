   @RequestMapping(value = { "/VAYANADOCUPLD" }, method = {
			org.springframework.web.bind.annotation.RequestMethod.POST })
			@ResponseBody
			public String VAYANADOCUPLD(HttpServletRequest request, HttpServletResponse response, HttpSession session)
			throws Exception {
			Object time = session.getAttribute("LogSession");
			String s1="";
			if (time == null) {
			return "redirect:";
			} else {
				
				
				VAYANADOCUPLDExcl VAYANADOCUPLD = new VAYANADOCUPLDExcl();
				boolean isMultipart = ServletFileUpload.isMultipartContent(request);
				
				String param1 = request.getParameter("param1");
				String param2 = request.getParameter("param2");
				String param3 = request.getParameter("param3");
				String param4 = request.getParameter("param4");
				String param5 = request.getParameter("param5");
